{
  "name": "cr-helper",
  "displayName": "CR Helper",
  "description": "A utility that helps you identify the most recent common ancestor of two git branches of your choice",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.crHelper"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.crHelper",
        "title": "CR Helper"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "eslint": "^5.13.0",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
